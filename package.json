    "@types/faker": "^4.1.2",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "cucumber": "^3.0.4",
    "cucumber-html-reporter": "^4.0.2",
    "mkdirp": "^0.5.1",
    "protractor": "^5.1.2",
    "protractor-cucumber-framework": "^4.0.8",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  },
  "repository": "https://github.com/igniteram/protractor-cucumber-typescript",
